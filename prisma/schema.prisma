generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id         Int       @id @default(autoincrement())
  login      String    @unique
  avatar_url String
  token      String?
  pokemons   Pokemos[]
}

model Pokemos {
  pokeId  Int    @id @default(autoincrement())
  name    String
  ability String
  user    Users? @relation(fields: [userId], references: [id])
  userId  Int?
}
